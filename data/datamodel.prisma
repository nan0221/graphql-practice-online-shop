type Product {
  id: ID! @unique
  name: String
  desc: String
  photo: String
  price: String
}


type Customer {
  email: String! @unique
  firstName: String
  lastName: String
  address: String
  phone: String
  password: String
  role: Role @default(value: "CUSTOMER")
  products: String
}

enum Role {
  CUSTOMER
  ADMIN
}